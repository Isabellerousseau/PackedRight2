<div class="map-container">

  <div
  class="top"
  class="positionmap"
  id="map"
  data-show-route="1"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
  >
  <%= render 'shared/navbar-new' %>
</div>

<div>
</div>
</div>


<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#driver_modal" hidden id="modalbtn">
  Launch demo modal
</button>
<!-- Modal -->
<div class="modal fade" id="driver_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

<div id="driver-box" data-driver-id="<%= @driver.id %>"></div>
